			-->
				<hasValue>U40.19.00.00; ""</hasValue>
				<!-- delivery service identifier -->
				<lineName lineNumber="023">COUNTY-LINE</lineName>
			</lineSelect>
			<lineSelect>
				<!--
				The following line occurs unconditionally.
			-->
				<lineName lineNumber="024">POSTCODE-LINE</lineName>
			</lineSelect>
